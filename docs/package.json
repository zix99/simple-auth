{
  "name": "simple-auth-docs",
  "version": "1.0.0",
  "description": "*Simple-Auth* is a designed to be an easy way to manage users in a multi-usecase scenario.",
  "main": "index.js",
  "scripts": {
    "dev": "vuepress dev ./",
    "build": "npm run build:vuepress && npm run build:spectacle",
    "build:vuepress": "vuepress build ./ && npm run",
    "build:spectacle": "spectacle -t .vuepress/dist/api ../pkg/swagdocs/swagger.json",
    "clean": "rm -rf .vuepress/dist",
    "deploy": "surge --project .vuepress/dist/ --domain simple-auth.surge.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mermaid": "^8.8.4",
    "spectacle-docs": "^1.1.0",
    "surge": "^0.21.7",
    "vuepress": "^1.7.1"
  }
}
